    <section id="contact">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <h3>Contact US</h3>            
          </div>
        </div>
      </div>
    </section>
    <div class="container">
      <div class="row">
        <div class="col-md-3 col-sm-3 wow bounceInLeft">
          <p class="mail"><i class="fa fa-map-marker"></i><br>
          <%= @company.address_line_1 %>, <br> <%= @company.address_line_2 %><br>
          <%= @company.city %>, <%= @company.state %>, <%= @company.country %>,<br><%= @company.postal_code %> </p>
        </div>
        <div class="col-md-3 col-sm-3 wow bounceInLeft">
          <p class="mail"><i class="fa fa-phone"></i><br>
          &nbsp; <%= @company.phone_number %></p>
        </div>
        <div class="col-md-3 col-sm-3 wow bounceInLeft">
          <p class="mail"><i class="fa fa-envelope"></i><br>
          <%= @company.email %><br>
          You can e-mail us your documents, mail them or either drop them
          off.</p>
        </div>
        <div class="col-md-3 col-sm-3 wow bounceInLeft">
          <div>
            <p class="mail"><i class="fa fa-clock-o"></i>
          Office Hours:</p>
          </div>
          <div class="office_text">
            <%= simple_format(@company.office_hours) %>
          </div>
        </div>
        <img alt="Quick book certified" class="img-responsive intro-img" src="images/col_new_clients.png">
      </div>
      <div class="row">
        <div class="col-md-12">
          <h3>Leave us a Message</h3>
        </div>
      </div>
      <div class="row">
        <%= form_for ContactForm.new do |f| %>
          <div class="col-md-12 col-sm-12 wow bounceInRight">
            <form action="#" class="frm" method="post">
              <div class="col-md-6">
                <h4><label for="Name">Full name *</label></h4> <%= f.text_field :name, required: true, style: "height: 50px; width: 100%;" %>
              </div>
              <div class="col-md-6">
                <h4><label for="Email">Email *</label></h4> <%= f.email_field :email, required: true, style: "height: 50px; width: 100%;" %>
              </div>
              <div class="col-md-12">
                <h4><label for="Message">Messages *</label></h4>
                <%= f.text_area :message, as: :text, required: true, placeholder: 'Your message here', style: "height: 150px; width: 100%;" %>
                <%= f.submit 'Send Message', class: "btn btn-primary btn-lg btn-block btn-biru" %>
              </div>
              <div class="hidden">
                <label for="nickname">nickname *</label>
                <%= f.text_field :nickname %>
              </div>
            </form>
          </div>
        <% end %>
      </div>
      <div class="row">
        <div class="col-md-12 footer-social-network">
          <p class="icon-contact">
          <!-- <a href="#"><i class="fa fa-weibo"></i></a> -->
           <a href="https://www.facebook.com/Fortaxcorp"><i class=
